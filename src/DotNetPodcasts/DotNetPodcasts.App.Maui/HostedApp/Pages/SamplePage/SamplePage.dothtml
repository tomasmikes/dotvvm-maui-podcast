@viewModel DotNetPodcasts.App.Maui.HostedApp.Pages.SamplePage.SamplePageViewModel, DotNetPodcasts.App.Maui
@masterPage Pages/MasterPage.dotmaster

<dot:Content ContentPlaceHolderID="MainContent">
    <dot:TextBox Type="Number" Text="{value: Op1}" /> + 
    <dot:TextBox Type="Number" Text="{value: Op2}" />
    {{value: Result}}
    <dot:Button Text="Add up" Click="{command: AddUp(Op1, Op2)}" />
    <dot:RouteLink RouteName="Default" Text="Return to root page"/>
</dot:Content>