@viewModel DotNetPodcasts.App.Web.Pages.SubscribedPodcasts.SubscribedPodcastsViewModel, DotNetPodcasts.App.Web
@masterPage Pages/MasterPage.dotmaster

<dot:Content ContentPlaceHolderID="MainContent">
    <header>
    </header>
    <dot:Repeater DataSource="{value: SubscribedPodcasts}" class="podcast-list">
        <ItemTemplate>
            <cc:PodcastCard Podcast="{value: _this}" IconClicked="{command: _root.ToggleSubscribe(_this)}" />
        </ItemTemplate>
        <EmptyDataTemplate>
            <h1>You are not subscribed to any podcast yet.</h1>
        </EmptyDataTemplate>
    </dot:Repeater>
</dot:Content>

